<launch>
  
  <node pkg="rbx2_ar_tags" name="ar_follower" type="ar_follower.py" clear_params="true" output="screen">
    <rosparam>
       rate: 10
       max_x: 20.0
       goal_x: 0.7
       x_threshold: 0.1
       y_threshold: 0.05
       y_scale: 5.0
       x_scale: 1.0
       max_angular_speed: 5.0
       min_angular_speed: 0.5
       max_linear_speed: 0.5
       min_linear_speed: 0.05
    </rosparam>
    
  </node>
</launch>
